<table>
  <tr>
    <th>ISBNコード</th><th>書名</th><th>価格</th>
    <th>出版社</th><th>刊行日</th><th>ダウンロード</th>
  </tr>
<% @books.each do |book| %>
  <tr>
<%
=begin
%>
  <tr style="background-color:<%= cycle('#FFffFF', '#FFff99') %>">
<%
=end
%>
    <td><%= book.isbn %></td>
    <td><%= book.title %></td>

<%
=begin
%>  
    <td style="background-color:<%= cycle('#FFffFF', '#FFff99') %>">
      <%= book.isbn %></td>
    <td style="background-color:<%= current_cycle %>">
      <%= book.title %></td>
<%
=end
%>
    <td><%= book.price %>円</td>
    <td><%= book.publish %></td>
    <td><%= book.published %></td>
    <td><%= book.dl %></td>
  </tr>
<% end %>
</table>
<%#= debug(@books) %>
